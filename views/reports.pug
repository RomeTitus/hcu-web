extends layout

block content

	div.area
		h2 Reports
			div.area
				//-hello	
				h1 Horses
		block content
	script(src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.min.js")
	

	canvas#myChart
	script.
			
			var ctx = document.getElementById('myChart').getContext('2d');
			var idata = JSON.parse('!{datai}');  
			var myChart = new Chart(ctx, {
			
				type: 'line',
				data: {
					labels: ['January', 'February ', 'March', 'April', 'May', 'June','July', 'August', 'September', 'October', 'November', 'December'],
					datasets: [{
						label: '# of Votes',
						data: idata,
						backgroundColor: [

							'rgba(255, 159, 64, 0.3)'
						],
						borderColor: [
							'rgba(255, 99, 132, 1)'
							
						],
						borderWidth: 1
					},{
						label: 'HEYYYY',
						data: [-1,-2,-1,-4],
						backgroundColor: [

							'rgba(0, 0, 0, 0.5)'
						],
						borderColor: [
							'rgba(0, 0, 0, 0)'
							
						],
						borderWidth: 1
					}]
				},
				options: {
					scales: {
						yAxes: [{
							ticks: {
								beginAtZero: true
							}
						}]
					}
				}
			});
			